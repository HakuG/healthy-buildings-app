<div class="row no-gutters">
<div class="col-md-4">
  <h3>Overall Health Score</h3>
  <h4 id="date">{{todaysDate}}</h4>
  <div class="muted-text score-container"><span class="overall-score">{{overall}}</span><span class="small-text"> / 60</span>
  <p>{{numsensors}} sensors reporting</p></div>

  <ul class="keys">
    <li><div class="key humid-key"></div>Humidity</li>
    <li><div class="key tc-key"></div>Thermal Comfort</li>
    <li><div class="key noise-key"></div>Noise</li>
    <li><div class="key aer-key"></div>Air Exchange Rate</li>
    <li><div class="key baseline-key"></div>Baseline Score</li>
    <li><div class="key overall-key"></div>Overall Health Score</li>
  </ul>
</div>

{{!-- {{building-page/center/donut building=building}} --}}
  <div class="col-md-8 main-donut-chart">
    {{building-page/center/radial width=150 height=150 class="medium" building=building baseline=baseline overall=overall}}
  </div>
</div>
