<div class="modal fade" id="detailsModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel">
  <div class="modal-dialog" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
        <h4 class="modal-title" id="myModalLabel">Modal title</h4>
      </div>
      {{!-- <div class="modal-body"> --}}
      <div class="row small-space">
        {{!-- <div class="small-space"></div> --}}
      </div>
      <div class="row">
        <div class="col-md-12" id="modalContent"></div>
        <div class="col-md-12" id="tcModalContent">
          {{!-- {{#if (eq tcModalShow 'true')}} --}}
            <h1>hello</h1>
          {{!-- {{/if}} --}}

        </div>
      </div>
      <div class="timeline-group">
        <div class="row small-space">
          <div class="col-md-12 center-container">
            <h4 id="modalDate">Date</h4>
          </div>
        </div>
        <div class="row">
          <div class="col-md-12 center-container">

            <!-- Controls -->
            <a class="left timeline-control left-arrow" role="button" {{action "leftArrow"}} id="leftArrow">
              <span class="glyphicon glyphicon-chevron-left" aria-hidden="true"></span>
              <span class="sr-only">Previous</span>
            </a>

            {{ui-slider ticks='1,2,3,4,5' tooltip='hide' changed='changed' selection="none" id="radialSlider"}}

            <a class="right timeline-control right-arrow" role="button" {{action "rightArrow"}} id="rightArrow">
              <span class="glyphicon glyphicon-chevron-right" aria-hidden="true"></span>
              <span class="sr-only">Next</span>
            </a>

          </div>
        </div>
        <div class="row">
          <div class="col-md-8 col-md-offset-2">
            <ul class="timeline-labels">
              <li class="firstDate">11/8/2015</li>
              <li class="secondDate">11/9/2015</li>
              <li class="thirdDate">11/10/2015</li>
              <li class="fourthDate">11/11/2015</li>
              <li class="fifthDate selected-date">11/12/2015</li>
            </ul>
          </div>
      </div>
    </div> {{!-- end timeline group --}}
    <div class="row small-space">
      {{!-- <div class="small-space"></div> --}}
    </div>
    <div class="modal-footer">
      <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
    </div>
    </div>
    </div>
    </div>

