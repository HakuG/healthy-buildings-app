<div class="container-fluid">
  <h2>Building Dashboard</h2>
  <h6><a {{action "editDashboard" on="click"}}><span class="glyphicon glyphicon-pencil" aria-hidden="true"></span> <span id="edit-link">Edit</span></a>
  <span class="float-right">{{buildings-dashboard/new createItem="createItem"}}</span></h6>
  <div class="row no-gutter clear-float">
    {{#each model as |building|}}
      <div class="col-md-4 building-card">{{buildings-dashboard building=building}}</div>
    {{/each}}
  </div>
</div>
